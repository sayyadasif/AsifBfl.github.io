@{
    ViewData["Title"] = "Account Level Dispatch Report";
}

<partial name="_FilterReport" />

<section class="section">
    <div class="row">
        <div class="col-lg-12">
            <div class="card">
                <div class="card-body">
                    <input type="hidden" value="accountLevelDispatchReportDatatable" id="reportName" />
                    <table id="accountLevelDispatchReportDatatable" class="table datatable" width="100%" cellspacing="0">
                        <thead>
                            <tr>
                                <th>Indent Number</th>
                                <th>Date of <br />Indent<br />(Approved by BFIL)</th>
                                <th>Date of <br />Indent <br />(Approved by IBL)</th>
                                <th>Date of <br />Dispatch</th>
                                <th>Date of <br />Dispatch report upload (By IBL)</th>
                                <th>Report <br />uploaded by (IBL User)</th>
                                <th>Date of <br />upload</th>
                                <th>IBL <br />Branch Code</th>
                                <th>IBL <br />Branch Name</th>
                                <th>BFIL<br />Branch code</th>
                                <th>CIF no</th>
                                <th>A/c stat</th>
                                <th>Status</th>
                            </tr>
                        </thead>
                    </table>

                </div>
            </div>
        </div>
    </div>
</section>

@section Scripts {
    <script src="~/js/report.js"></script>
}
