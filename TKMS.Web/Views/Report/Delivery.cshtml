@{
    ViewData["Title"] = "Delivery Report";
}

<partial name="_FilterReport" />

<section class="section">
    <div class="row">
        <div class="col-lg-12">
            <div class="card">
                <div class="card-body">
                    <input type="hidden" value="deliveryReportDatatable" id="reportName" />

                    <table id="deliveryReportDatatable" class="table datatable" width="100%" cellspacing="0">
                        <thead>
                            <tr>
                                <th>Indent<br />Number</th>
                                <th>Way <br />Bill No</th>
                                <th>Dispatch<br />Date</th>
                                <th>Status from<br />Courier</th>
                                <th>Delivery<br />Date</th>
                                <th>Receiver<br />Name</th>
                                <th>Courier<br />Name</th>
                                <th>Reference<br />Number</th>
                            </tr>
                        </thead>
                    </table>

                </div>
            </div>
        </div>
    </div>
</section>

@section Scripts {
    <script src="~/js/report.js"></script>
}
